@page "/create/define-exercise"
@using ExerciseEngine;
@inject AppState appState

<Creation_Header/>

<MudButton Variant="Variant.Filled" Color="Color.Warning" StartIcon="@Icons.Filled.KeyboardReturn" Href="create/confirm-variables">Return to variables</MudButton>
<MudButton Variant="Variant.Filled" Color="Color.Tertiary" Class="ml-auto" EndIcon="@Icons.Material.Filled.Send" Href="create/confirm-exercise">Continue</MudButton>

@code {
    protected override void OnParametersSet() {
        base.OnParametersSet();
        appState.CreationPhase = 5; 
    }
}
