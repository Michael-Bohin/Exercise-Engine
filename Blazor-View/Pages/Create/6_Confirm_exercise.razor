@page "/create/confirm-exercise"
@using ExerciseEngine;
@inject AppState appState

<Creation_Header/>

<MudButton Variant="Variant.Filled" Color="Color.Warning" StartIcon="@Icons.Filled.KeyboardReturn" Href="create/define-exercise">Return to editing</MudButton>
